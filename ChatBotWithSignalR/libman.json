{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "library": "select2@4.1.0-rc.0",
      "destination": "wwwroot/lib/select2/",
      "files": [
        "css/select2.css",
        "css/select2.min.css",
        "js/select2.js",
        "js/select2.min.js"
      ]
    },
    {
      "library": "qrcode@1.5.1",
      "destination": "wwwroot/lib/qrcode/"
    },
    {
      "library": "jquery@3.7.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.js",
        "jquery.min.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/bootstrap.css",
        "css/bootstrap.min.css",
        "css/bootstrap.min.css.map",
        "js/bootstrap.bundle.js",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map",
        "js/bootstrap.js",
        "js/bootstrap.min.js",
        "js/bootstrap.min.js.map"
      ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validation/dist/",
      "files": [
        "jquery.validate.js",
        "jquery.validate.min.js",
        "additional-methods.js",
        "additional-methods.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.js",
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "font-awesome@6.4.0",
      "destination": "wwwroot/lib/font-awesome/"
    }
  ]
}