{
    "version": "1.0",
    "defaultProvider": "cdnjs",
    "libraries": [
        {
            "provider": "cdnjs",
            "library": "font-awesome@6.4.0",
            "destination": "wwwroot/lib/font-awesome/"
        },
        {
            "provider": "cdnjs",
            "library": "toastr.js@2.1.4",
            "destination": "wwwroot/lib/toastr.js/"
        },
        {
            "provider": "cdnjs",
            "library": "select2@4.1.0-rc.0",
            "destination": "wwwroot/lib/select2/",
            "files": [
                "css/select2.css",
                "css/select2.min.css",
                "js/select2.js",
                "js/select2.min.js"
            ]
        },
        {
            "provider": "cdnjs",
            "library": "qrcode@1.5.1",
            "destination": "wwwroot/lib/qrcode/"
        },
        {
            "provider": "cdnjs",
            "library": "jquery@3.7.0",
            "destination": "wwwroot/lib/jquery/",
            "files": [
                "jquery.js",
                "jquery.min.js",
                "jquery.min.map"
            ]
        },
        {
            "provider": "cdnjs",
            "library": "bootstrap@5.3.0",
            "destination": "wwwroot/lib/bootstrap/",
            "files": [
                "css/bootstrap.css",
                "css/bootstrap.min.css",
                "css/bootstrap.min.css.map",
                "js/bootstrap.bundle.js",
                "js/bootstrap.bundle.min.js",
                "js/bootstrap.bundle.min.js.map",
                "js/bootstrap.js",
                "js/bootstrap.min.js",
                "js/bootstrap.min.js.map"
            ]
        }
    ]
}